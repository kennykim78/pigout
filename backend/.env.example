# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Server Configuration
PORT=3001
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# AI Configuration
GEMINI_API_KEY=your_gemini_api_key

# OpenData API Keys (공공데이터포털)
# 신청: https://www.data.go.kr/
MFDS_API_KEY=your_mfds_api_key
RECIPE_DB_API_KEY=your_recipe_db_api_key
# 실패/미사용 API 자리 (필요 시 활성화)
NUTRITION_API_KEY=your_nutrition_api_key
FUNCTIONAL_FOOD_API_KEY=your_functional_food_api_key
DISEASE_INFO_API_KEY=your_disease_info_api_key

# Feature Flags / Logging
ENABLE_MEDICINE_INTERACTIONS=true
ENABLE_RECIPE_FETCH=true
LOG_LEVEL=debug
